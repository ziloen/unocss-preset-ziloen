{
  "name": "unocss-preset-ziloen",
  "type": "module",
  "version": "0.0.4",
  "description": "<description>",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp --commit --no-push --tag && pnpm publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:update": "vitest --update",
    "play": "vite playground",
    "deploy": "vite build playground"
  },
  "peerDependencies": {
    "@unocss/core": "< 1"
  },
  "devDependencies": {
    "@babel/types": "^7.23.0",
    "@types/node": "^20.8.5",
    "@vitejs/plugin-vue": "^4.4.0",
    "@ziloen/eslint-config": "^0.0.110",
    "bumpp": "^9.2.0",
    "eslint": "^8.51.0",
    "typescript": "5.2.2",
    "unbuild": "^2.0.0",
    "unocss": "^0.56.5",
    "vite": "^4.4.11",
    "vitest": "^0.34.6"
  },
  "eslintConfig": {
    "extends": [
      "@ziloen/eslint-config"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    }
  }
}
